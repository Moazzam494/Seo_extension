{
  "manifest_version": 3,
  "name": "SEO Keyword Analyzer",
  "version": "1.0",
  "description": "Analyzes SEO metrics for keywords using a custom Python backend.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "http://127.0.0.1:5000/*",
    "http://localhost:5000/*"
  ]
}